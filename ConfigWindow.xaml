<Window x:Class="SelectUnknown.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SelectUnknown"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        TextElement.Foreground="{DynamicResource MaterialDesign.Brush.Foreground}"
        Background="{DynamicResource MaterialDesign.Brush.Background}"
        TextElement.FontWeight="Medium"
        TextElement.FontSize="14"
        Title="ConfigWindow" Height="630" Width="920">

    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="190"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <!--侧栏-->
        <StackPanel Grid.Column="0" Background="#FFF4E6DB">
            <StackPanel Margin="15" >
                <Button Content="常规设置"></Button>
                <Button Content="热键设置" Margin="0,15"></Button>
            </StackPanel>
        </StackPanel>
        <!--主区域-->
        <StackPanel Grid.Column="1">
            <Button Content="test"></Button>
        </StackPanel>
    </Grid>
</Window>
